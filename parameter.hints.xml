<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="SQL">
        <added pattern="\sprintf(format,args,_)" />
        <added pattern="\Doctrine\ORM\QueryBuilder.andWhere(where)" />
        <added pattern="\Doctrine\ORM\QueryBuilder.where(predicates)" />
        <added pattern="\Doctrine\ORM\QueryBuilder.groupBy(groupBy)" />
        <added pattern="\Doctrine\ORM\QueryBuilder.select(select)" />
        <added pattern="\Doctrine\ORM\Query\Expr.neq(x,y)" />
        <added pattern="\Doctrine\ORM\Query\Expr.literal(literal)" />
        <added pattern="\Symfony\Component\HttpFoundation\ParameterBag.get(path,default,deep)" />
        <added pattern="\Symfony\Component\EventDispatcher\EventDispatcher.dispatch(eventName,event)" />
        <added pattern="\Doctrine\DBAL\Migrations\AbstractMigration.addSql(sql,params,types)" />
      </blacklist>
    </blacklists>
  </component>
</application>